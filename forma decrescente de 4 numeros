#include <stdio.h>

void main(){
    int n1,n2,n3,n4;

    printf("Diz o primeiro numero: ");
    scanf("%d", &n1);
    printf("Diz o segundo numero: ");
    scanf("%d", &n2);
    printf("Diz o terceiro numero: ");
    scanf("%d", &n3);
    printf("Diz o quarto numero: ");
    scanf("%d", &n4);

    if (n1>n2 && n2>n3 && n3>n4)
    printf("%d,%d,%d,%d",n1,n2,n3,n4);

    if (n1>n2 && n2>n4 && n4>n3)
    printf("%d,%d,%d,%d",n1,n2,n4,n3);

    if (n1>n3 && n3>n2 && n2>n4)
    printf("%d,%d,%d,%d",n1,n3,n2,n4);

    if (n1>n3 && n3>n4 && n4>n2)
    printf("%d,%d,%d,%d",n1,n3,n4,n2);

    if (n1>n4 && n4>n3 && n3>n2)
    printf("%d,%d,%d,%d",n1,n4,n3,n2);

    if (n1>n4 && n4>n2 && n2>n3)
    printf("%d,%d,%d,%d",n1,n4,n2,n3);





    if (n2>n1 && n1>n3 && n3>n4)
    printf("%d,%d,%d,%d",n2,n1,n3,n4);

    if (n2>n1 && n1>n4 && n4>n3)
    printf("%d,%d,%d,%d",n2,n1,n4,n3);

    if (n2>n3 && n3>n1 && n1>n4)
    printf("%d,%d,%d,%d",n2,n3,n1,n4);

    if (n2>n3 && n3>n4 && n4>n1)
    printf("%d,%d,%d,%d",n2,n3,n4,n1);

    if (n2>n4 && n4>n1 && n1>n3)
    printf("%d,%d,%d,%d",n2,n4,n1,n3);

    if (n2>n4 && n4>n3 && n3>n1)
    printf("%d,%d,%d,%d",n2,n4,n3,n1);





    if (n3>n1 && n1>n2 && n2>n4)
    printf("%d,%d,%d,%d",n3,n1,n2,n4);

    if (n3>n1 && n1>n4 && n4>n2)
    printf("%d,%d,%d,%d",n3,n1,n4,n2);

    if (n3>n2 && n2>n1 && n1>n4)
    printf("%d,%d,%d,%d",n3,n2,n1,n4);

    if (n3>n2 && n2>n4 && n4>n1)
    printf("%d,%d,%d,%d",n3,n2,n4,n1);

    if (n3>n4 && n4>n2 && n2>n1)
    printf("%d,%d,%d,%d",n3,n4,n2,n1);

    if (n3>n4 && n4>n1 && n1>n2)
    printf("%d,%d,%d,%d",n3,n4,n1,n2);




    if (n4>n1 && n1>n2 && n2>n3)
    printf("%d,%d,%d,%d",n4,n1,n2,n3);

    if (n4>n1 && n1>n3 && n3>n2)
    printf("%d,%d,%d,%d",n4,n1,n3,n2);

    if (n4>n2 && n2>n1 && n1>n3)
    printf("%d,%d,%d,%d",n4,n2,n1,n3);

    if (n4>n2 && n2>n3 && n3>n1)
    printf("%d,%d,%d,%d",n4,n2,n3,n1);

    if (n4>n3 && n3>n2 && n2>n1)
    printf("%d,%d,%d,%d",n4,n3,n2,n1);

    if (n4>n3 && n3>n1 && n1>n2)
    printf("%d,%d,%d,%d",n4,n3,n1,n2);

}
